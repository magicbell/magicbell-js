    "@types/ramda": "^0.28.0",
    "@types/rosie": "^0.0.45",
    "babel-loader": "^8.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rosie": "^2.1.1",
    "sinon": "^13.0.2",
    "size-limit": "^8.2.6",
    "twind": "^0.16.19",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
