    "directory": "packages/react-headless"
  },
  "bugs": {
    "url": "https://github.com/magicbell/magicbell-js/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean build:*",
    "build:dev": "vite build -c ../../scripts/vite/vite.config.js",
