  "license": "SEE LICENSE IN LICENSE",
  "source": "./src/index.ts",
  "main": "src/index.ts",
  "module": "dist/magicbell-utils.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "/dist",
    "/src",
    "LICENSE"
  ],
  "homepage": "https://magicbell.com",
  "keywords": [
    "magicbell",
    "codegen"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:magicbell-io/magicbell-js.git",
