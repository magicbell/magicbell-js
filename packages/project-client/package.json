{
  "name": "@magicbell/project-client",
  "version": "0.8.0",
  "keywords": [
    "typescript",
    "sdk",
    "api",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magicbell/magicbell-js.git",
    "directory": "packages/project-client"
  },
  "author": "MagicBell",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "source": "./src/index.ts",
  "types": "./dist/commonjs/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tshy",
    "build:docs": "tsx ../../scripts/liblab-docs.ts",
    "codegen": "tsx ../../scripts/liblab-gen.ts --spec https://www.magicbell.com/docs/api/openapi.project.json",
    "start": "tshy --watch"
  },
  "dependencies": {
    "zod": "3.24.1"
  },
  "devDependencies": {
    "mdast": "^3.0.0",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-frontmatter": "^2.0.1",
    "mdast-util-to-markdown": "^2.1.2",
    "micromark-extension-frontmatter": "^2.0.0",
    "typescript": "^5.7.3",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "docs": {
    "name": "JavaScript Project Client"
  },
  "tshy": {
    "project": "./tsconfig.build.json",
    "exports": "./src/index.ts"
  }
}
