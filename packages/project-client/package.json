{
  "name": "@magicbell/project-client",
  "version": "0.2.0",
  "description": "OpenAPI 3.1.0 Specification for MagicBell API.",
  "keywords": [
    "typescript",
    "sdk",
    "api",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magicbell/magicbell-js.git",
    "directory": "packages/project-client"
  },
  "author": "MagicBell",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "source": "./src/index.ts",
  "types": "./dist/commonjs/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "tshy": {
    "project": "./tsconfig.build.json",
    "exports": "./src/index.ts"
  },
  "scripts": {
    "build": "tshy",
    "codegen": "tsx scripts/build.ts",
    "start": "rm -rf dist/ && tsc -w"
  },
  "dependencies": {
    "zod": "3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.5.3"
  },
  "type": "module"
}
