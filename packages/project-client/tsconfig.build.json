{
  "extends": "../../tsconfig.build.json",
  "include": ["src"],
  "exclude": ["src/client/@custom_types"],
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "target": "es2018",
    "baseUrl": "./",
    "declarationMap": true,
    "lib": ["es2018", "dom"],
    "noEmit": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": true
  }
}
