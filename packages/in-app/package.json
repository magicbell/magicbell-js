{
  "name": "@magicbell/in-app",
  "version": "0.2.5",
  "description": "MagicBell web components for in-app notifications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magicbell/magicbell-js.git",
    "directory": "packages/in-app"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/*",
      "default": "./dist/*"
    }
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:cem && npm run build:react && npm run build:components && npm run build:css",
    "build:watch": "tsc --watch --project tsconfig.json",
    "build:components": "tsc --project tsconfig.json",
    "build:react": "tsx scripts/build-react.ts",
    "build:css": "tsx scripts/build-css.ts",
    "build:cem": "cem analyze --litelement --outdir dist --globs \"src/*.ts\"",
    "build:cem:watch": "cem analyze --litelement --outdir dist --globs \"src/*.ts\" --watch",
    "start": "npm run build:watch",
    "clean": "rimraf dist",
    "lint": "npm run lint:lit-analyzer && npm run lint:biome",
    "lint:biome": "npx @biomejs/biome check --write .",
    "lint:lit-analyzer": "lit-analyzer src/*.ts",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "BROWSERS=chromium wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch"
  },
  "keywords": [
    "magicbell",
    "components",
    "web-components",
    "typescript",
    "lit"
  ],
  "author": "MagicBell, inc",
  "license": "MIT",
  "dependencies": {
    "@lit/react": "^1.0.5",
    "@magicbell/user-client": "0.6.0",
    "lit": "^3.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.7",
    "@biomejs/biome": "1.9.2",
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.8",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.2",
    "@web/dev-server-rollup": "^0.6.4",
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-playwright": "^0.11.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "babel": "^6.23.0",
    "babel-preset-solid": "^1.9.0",
    "command-line-args": "^5.2.1",
    "lit-analyzer": "^2.0.3",
    "mime-types": "^2.1.35",
    "pascal-case": "^4.0.0",
    "react": "npm:@pika/react@16.13.1",
    "react-dom": "npm:@pika/react-dom@16.13.1",
    "rimraf": "^5.0.7",
    "rollup": "^2.73.0",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-svelte": "^7.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "solid-js": "^1.9.1",
    "svelte": "^4.2.18",
    "ts-lit-plugin": "^2.0.2",
    "typescript": "^5.6.2",
    "vue": "^3.5.11"
  }
}
