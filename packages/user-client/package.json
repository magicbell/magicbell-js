{
  "name": "@magicbell/user-client",
  "version": "0.2.0",
  "description": "OpenAPI 3.1.0 Specification for MagicBell API.",
  "keywords": [
    "typescript",
    "sdk",
    "api",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magicbell/magicbell-js.git",
    "directory": "packages/user-client"
  },
  "author": "MagicBell",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/commonjs/index.js"
    }
  },
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "source": "./src/index.ts",
  "types": "./dist/commonjs/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --outDir dist/commonjs",
    "build:esm": "tsc --project tsconfig.build.json --module esnext --outDir dist/esm",
    "codegen": "tsx scripts/build.ts",
    "start": "rm -rf dist/ && tsc -w",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.5.3"
  }
}
